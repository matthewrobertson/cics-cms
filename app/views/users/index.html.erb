<h1>My Associates</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Member Since</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <%if can? :edit, user %>
              <td><%= link_to user.full_name, user_path(user) %></td>
            <%else%>
                <td><%= user.full_name %></td>
        <%end%>
        <td><%= user.email %></td>
        <td><%= user.created_at.to_s(:slash_date) %></td>
        <td><%= link_to 'Message',  "#", :class => 'btn btn-mini' %></td>
      </tr>
    <% end %>
  </tbody>
</table>


